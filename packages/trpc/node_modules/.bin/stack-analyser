#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/github/nextjs/monorepo-starter/node_modules/.pnpm/@specfy+stack-analyser@1.9.0/node_modules/@specfy/stack-analyser/dist/node_modules:/root/github/nextjs/monorepo-starter/node_modules/.pnpm/@specfy+stack-analyser@1.9.0/node_modules/@specfy/stack-analyser/node_modules:/root/github/nextjs/monorepo-starter/node_modules/.pnpm/@specfy+stack-analyser@1.9.0/node_modules/@specfy/node_modules:/root/github/nextjs/monorepo-starter/node_modules/.pnpm/@specfy+stack-analyser@1.9.0/node_modules:/root/github/nextjs/monorepo-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/github/nextjs/monorepo-starter/node_modules/.pnpm/@specfy+stack-analyser@1.9.0/node_modules/@specfy/stack-analyser/dist/node_modules:/root/github/nextjs/monorepo-starter/node_modules/.pnpm/@specfy+stack-analyser@1.9.0/node_modules/@specfy/stack-analyser/node_modules:/root/github/nextjs/monorepo-starter/node_modules/.pnpm/@specfy+stack-analyser@1.9.0/node_modules/@specfy/node_modules:/root/github/nextjs/monorepo-starter/node_modules/.pnpm/@specfy+stack-analyser@1.9.0/node_modules:/root/github/nextjs/monorepo-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@specfy/stack-analyser/dist/cli.js" "$@"
else
  exec node  "$basedir/../@specfy/stack-analyser/dist/cli.js" "$@"
fi
